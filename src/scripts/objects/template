import * as THREE from 'three/build/three.module';

class ArrowA extends THREE.Group {
    constructor(params) {
        params = {
            ...params
        }
        super(params)
        this.setup_components([
            {
                name: "MeshComponent",
                ref: "arrow",
                enabled: true,
                params: {
                    geometry: [
                        "BoxBufferGeometry",
                        [1, 1, 1]
                    ],
                    material: ["MeshBasicMaterial", {
                        color: "#ff0044"
                    }],
                    position: [0, 0, 0],
                },
            },
        ])
        console.log(params)
        super(params)
    }
    on_tick(time_stats) {
       
    }
}

export default ArrowA